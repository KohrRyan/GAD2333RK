using UnityEngine;
using System.Collections;

public class Helloworld: MonoBehaviour {

	public string name;


	public void Main ()
	{
		SayHello (name);
		SayGoodbye ();
	}
	// Use this for initialization
	void Start () 
	{

	
	}
	
	// Update is called once per frame
	void Update () 
	{

	
	}
	public void SayHello (string nme)
	{
		print ("Hello, " + nme);
	}
	public void SayGoodbye ()
	{
		print ("Cya later");
	}
}
